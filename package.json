{
  "name": "browster",
  "version": "1.0.0",
  "description": "A web browser with tab treeview built on Electron",
  "main": "main.js",
  "scripts": {
    "start": "npm-run-all --parallel electron lint",
    "electron": "electron main.js",
    "test": "karma start tests/karma.conf.js",
    "lint": "node_modules/jshint/bin/jshint --verbose --reporter=node_modules/jshint-stylish ./",
    "clean": "rm -Rf node_modules"
  },
  "jshintConfig": {
    "esversion": 6,
    "asi": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/timurtu/browser.git"
  },
  "keywords": [
    "Electron",
    "Web",
    "Browser",
    "Node"
  ],
  "author": "Timur Uzel <timurt.uzel@gmail.com> (https://github.com/timurtu)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/timurtu/browser/issues"
  },
  "homepage": "https://github.com/timurtu/browser#readme",
  "devDependencies": {
    "electron-compilers": "^3.0.0",
    "electron-prebuilt": "^0.37.7",
    "electron-prebuilt-compile": "^0.37.7",
    "install": "^0.6.1",
    "jasmine": "^2.4.1",
    "jshint": "^2.9.2",
    "jshint-stylish": "^2.1.0",
    "karma": "^0.13.22",
    "karma-electron-launcher": "^0.1.0",
    "karma-mocha": "^0.2.2",
    "mocha": "^2.4.5",
    "npm-run-all": "^1.7.0"
  },
  "dependencies": {
    "bootstrap": "^4.0.0-alpha.2",
    "electron-compile": "^3.0.1",
    "font-awesome": "^4.6.1",
    "jquery": "^2.2.3",
    "tether": "^1.2.2"
  }
}
